{
  "name": "nest-client",
  "version": "0.5.0",
  "description": "nest.js client stub library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist node_modules/@beenotung/tslib/index.*",
    "prebuild": "npm run clean",
    "build": "gen-index && tsc -p .",
    "prelint": "npm run format",
    "lint": "tslint -p .",
    "format": "prettier --write \"src/**/*.ts\"",
    "postformat": "tslint -p . --fix",
    "pretest": "run-s build lint",
    "test": "ts-node test/meta-test.ts"
  },
  "keywords": [
    "client-side",
    "library",
    "nest.js",
    "http",
    "rest",
    "restful"
  ],
  "author": "Beeno Tung",
  "license": "BSD-2-Clause",
  "devDependencies": {
    "@beenotung/gen-index": "^1.0.8",
    "@types/node": "^10.5.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.7",
    "rimraf": "^2.6.2",
    "tslint": "^5.9.1",
    "tslint-eslint-rules": "^5.3.1",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "@beenotung/tslib": "^14.20.0",
    "axios": "^0.18.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beenotung/nest-client.git"
  },
  "bugs": {
    "url": "https://github.com/beenotung/nest-client/issues"
  },
  "homepage": "https://github.com/beenotung/nest-client#readme"
}
