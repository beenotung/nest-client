{
  "name": "nest-client",
  "version": "1.0.1",
  "description": "nest.js client stub library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist node_modules/@beenotung/tslib/index.*",
    "prebuild": "npm run clean",
    "build": "gen-index && tsc -p .",
    "postbuild": "./post-build",
    "format": "prettier --write \"{src,test}/**/*.ts\" && format-json-cli",
    "pretest": "run-s build",
    "test": "ts-node test/meta-test.ts"
  },
  "keywords": [
    "client-side",
    "library",
    "nest.js",
    "http",
    "rest",
    "restful"
  ],
  "author": {
    "name": "Beeno Tung",
    "email": "aabbcc1241@yahoo.com.hk",
    "url": "https://beeno-tung.surge.sh"
  },
  "license": "BSD-2-Clause",
  "devDependencies": {
    "@beenotung/gen-index": "^1.2.1",
    "@types/node": "^18.11.11",
    "format-json-cli": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@beenotung/tslib": "^20.3.0",
    "axios": "^1.2.1"
  },
  "repository": {
    "type": "git",
